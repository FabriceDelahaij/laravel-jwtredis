{
    "name": "sametsahindogan/laravel-jwtredis",
    "description": "This package allows JWT-authenticated users to be stored and management in Redis with\ntheir roles, permissions, statuses, and anything you want.",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "tymon/jwt-auth": "1.0.*",
        "predis/predis": "^1.1",
        "spatie/laravel-permission": "^3.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "sametsahindogan",
            "email": "sametsahindogan@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Sametsahindogan\\JWTRedis\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "JWTAuth": "Sametsahindogan\\JWTRedis\\Facades\\JWTAuth"
            },
            "providers": [
                "Sametsahindogan\\JWTRedis\\JWTRedisServiceProvider"
            ]
        }
    }
}
